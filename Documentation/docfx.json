{
  "metadata": [
    {
      "src": [
        {
          "files": [
            "**.csproj"
          ],
          "exclude": [ "**/bin/**", "**/obj/**" ],
          "src": "../"
        }
      ],
      "dest": "api",
      "disableGitFeatures": false,
      "disableDefaultFilter": false
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "api/**.yml",
          "api/index.md"
        ]
      },
      {
        "files": [
          "articles/**.md",
          "articles/**/toc.yml",
          "toc.yml",
          "*.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "apidoc/**.md"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "dest": "../docs",
    "globalMetadataFiles": [],
    "globalMetadata": {
      "_appTitle": "Litdex.Security.RNG",
      "_description": "A collection of Pseudo Random Number Generator (PRNG) for .NET",
      "_enableSearch": true,
      "_enableNewTab": true,
      "_appFooter": "Developed by <a href='https://github.com/Shiroechi/Litdex.Security.RNG'>Shiroechi</a> and <a href='https://github.com/Shiroechi/Litdex.Security.RNG/graphs/contributors'>contributors</a> / Licensed under <a href='https://github.com/Shiroechi/Litdex.Security.RNG/blob/main/LICENSE'>MIT</a> / Website generated by <a href='https://dotnet.github.io/docfx/'>DocFX</a>"
    },
    "fileMetadataFiles": [],
    "template": [
      "default"
    ],
    "postProcessors": [],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false
  }
}